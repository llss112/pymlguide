    "    plt.plot([0,1],[0,1],'k-',label='Random')\n",
    "\n",
    "    start,end=plt.xlim()\n",
    "    plt.xlim(0,1);plt.ylim(0,1)\n",
    "    plt.xlabel('FPR(1-Specificity)');plt.ylabel('TPR(Recall)')\n",
    "    plt.legend()\n",
    "\n",
